{
  "version": 3,
  "overrides": {},
  "routes": [
    {
      "status": 307,
      "headers": {
        "Location": "/ssr"
      },
      "src": "/redirectToSSR"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "(?<url>/blog/.*)",
      "dest": "/__nitro--blog?url=$url"
    },
    {
      "src": "/(.*)",
      "dest": "/__nitro"
    }
  ]
}